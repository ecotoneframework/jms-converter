{
  "name": "ecotone/jms-converter",
  "license": "MIT",
  "homepage": "https://docs.ecotone.tech/",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Dariusz Gafka",
      "email": "dgafka.mail@gmail.com"
    }
  ],
  "keywords": [
    "messaging",
    "ecotone",
    "serializer",
    "integration",
    "json",
    "xml",
    "converter"
  ],
