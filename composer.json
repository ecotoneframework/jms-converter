  "require": {
    "php": "^8.0",
    "ecotone/ecotone": "~1.32.0",
    "jms/serializer": "^3.17",
    "symfony/cache": "^5.4|^6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "behat/behat": "^3.10",
    "phpstan/phpstan": "^1.7"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.32.0-dev"
    }
  }
